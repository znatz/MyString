#ifndef MyStringH
#define MyStringH
#include <vcl.h>

class MyString {

public:
	// Constructor
	MyString();
	MyString(UnicodeString param1);
	MyString(const MyString& obj);

	// Get and Set
	UnicodeString GetStr();
	void SetStr(UnicodeString param1);

	// Operator
	MyString& operator-(UnicodeString param1);

private:
	UnicodeString m_ustr;
};

#endif /* #ifndef MyStringH */
